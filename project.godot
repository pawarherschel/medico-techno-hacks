[application]

config/name="MedicoTechnoHacks"
run/main_scene="res://scenes/menu/menu.tscn"
boot_splash/show_image=false
boot_splash/image="res://assets/splash.png"
boot_splash/bg_color=Color( 0.0862745, 0.0862745, 0.0862745, 1 )
config/icon="res://assets/sprites/icon.png"
config/version="0.0.1"

[autoload]

Game="*res://addons/game-template/game.gd"
Transitions="*res://addons/game-template/transition/transition.tscn"
Utils="*res://addons/game-template/utils.gd"
PluginSettings="*res://addons/github-integration/scripts/PluginSettings.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"
UserData="*res://addons/github-integration/scripts/user_data.gd"
RestHandler="*res://addons/github-integration/scenes/RestHandler.tscn"

[category]

property_name=1

[display]

window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/game-template/plugin.cfg", "res://addons/github-integration/plugin.cfg", "res://addons/ui_design_tool/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]
singletons_disabled=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

